{
  "manifest_version": 3,
  "name": "Google Calendar EID Copier",
  "version": "1.0.0",
  "description": "GoogleカレンダーのイベントIDをワンクリックでコピーできるChrome拡張機能",
  "permissions": [
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://calendar.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
